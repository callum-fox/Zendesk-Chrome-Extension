{
  "name": "zendesk helper",
  "version": "0",
  "description": "makes life easier",
  "background": {"page": "background.html"},
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png"
  },
  "content_scripts": [ {
    "js": [ "jquery-3.6.0.min.js", "background.js" ],
    "css": ["customStyles.css"],
    "matches": [ "*://junipereducation.zendesk.com/*"]
  }]
}